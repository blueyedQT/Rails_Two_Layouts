<head>
  <title>Two Layouts</title>
  <%= stylesheet_link_tag 'two_column', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'two_column', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<h1>This is the two_column layout!</h1>
		<div class="row">
			<div id="left_column" class="col-xs-7">
				<%= yield :left_column %>
			</div>
			<div class="col-xs-4">
				<% yield %>
			</div>
		</div>
</body>